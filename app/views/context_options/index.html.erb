<h1>Listing context_options</h1>

<table>
  <tr>
    <th>Context</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @context_options.each do |context_option| %>
  <tr>
    <td><%= context_option.context_id %></td>
    <td><%= context_option.name %></td>
    <td><%= link_to 'Show', context_option %></td>
    <td><%= link_to 'Edit', edit_context_option_path(context_option) %></td>
    <td><%= link_to 'Destroy', context_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Context option', new_context_option_path %>
